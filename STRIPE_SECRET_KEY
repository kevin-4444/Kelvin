import time
import os

def mensaje_de_amor():
    mensaje = "TE AMO"
    for i in range(len(mensaje)):
        print(" " * (len(mensaje) - i - 1) + mensaje[:i+1])
        time.sleep(0.5)
        os.system("cls" if os.name == "nt" else "clear")
    print(mensaje)
    time.sleep(2)

mensaje_de_amor()
