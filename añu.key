def generar_corazon(tamaño):
    for i in range(tamaño):
        for j in range(tamaño):
            if (i - tamaño // 2) ** 2 + (j - tamaño // 2) ** 2 <= (tamaño // 2) ** 2:
                print("*", end=" ")
            else:
                print(" ", end=" ")
        print()

